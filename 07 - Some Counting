print("Counting 0 to 50:")
for i in range(0, 51, 1):
    print(i, end=" ")
print("\n")
print("Counting down 50 to 0:")
for i in range(50, -1, -1):
    print(i, end=" ")
print("\n")
print("Counting 30 to 50:")
for i in range(30, 51, 1):
    print(i, end=" ")
print("\n")
print("Counting down 50 to 10 (step 2):")
for i in range(50, 9, -2):
    print(i, end=" ")
print("\n")
print("Counting 100 to 200 (step 5):")
for i in range(100, 201, 5):
    print(i, end=" ")
